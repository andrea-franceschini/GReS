<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->

<Problem>

  <simParam fileName = "Input/simParam.xml" />

  <Output
          saveHistory = "1"
          outputFile = "Output/Results"
          matFileName = "Output/results"
          printTimes = "1,4,8"
          />

  <Domain>
    <Geometry fileName = "Input/ReservoirTest_Hexa.msh" />
    <Gauss nGP = "2" />
    <Materials fileName = "Input/materials.xml" />
    <BoundaryConditions fileName = "Input/boundaryConditions.xml" />

    <Solver>
      <BiotFullySaturated>
        <Poromechanics targetRegions = "1,2,3" />
        <SinglePhaseFlowFVTPFA targetRegions = "2,3" />
      </BiotFullySaturated>
    </Solver>

  </Domain>
</Problem>
