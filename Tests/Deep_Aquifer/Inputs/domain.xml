<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->
<Domains>
  <Domain>

    <Name> DeepAquifer </Name>    
    <Geometry> Inputs/ReservoirTest_Hexa.msh </Geometry>

    <ModelType>
      SinglePhaseFlow_FVTPFA
      Poromechanics_FEM
    </ModelType>

    <SimParameters> Inputs/simParam.xml </SimParameters>
    <DoFManager> Inputs/dof.dat </DoFManager>
    <Gauss> 2 </Gauss>
    <Material> Inputs/materials.xml </Material>
    <BoundaryConditions> Inputs/boundaryConditions.xml </BoundaryConditions>

    <OutState
        saveHistory = "1"
        outputFile = "Results"
        printTimes = "1,4,8"
    />
    
  </Domain>
</Domains>
