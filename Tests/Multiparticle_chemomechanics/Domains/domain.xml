<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->
<struct>
  <Domain>
    <Name> TwoSpheres </Name>
    <Geometry> Mesh/two_spheres.vtk </Geometry>
    <ModelType> SinglePhaseFlow_FEM Poromechanics_FEM </ModelType>
    <SimParameters> simParam.dat </SimParameters>
    <Gauss> 1 </Gauss>
    <Material> Materials/materialsList_Zhang2007.dat </Material>
    <BoundaryConditions count="3">
        <File> BCs/LeftSphere_cmax.dat </File>
        <File> BCs/RightSphere_cmax.dat </File>
        <File> BCs/FixLeftSphere.dat </File>
    </BoundaryConditions>
    <OutState outFile="outTime.dat"  print="1" />
  </Domain>
</struct>
