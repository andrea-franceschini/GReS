<?xml version="1.0" encoding="UTF-8"?>
<SimulationInput>
  <!-- Domain 1 -->
  <Domain>
    <Name>SingleCrackModel</Name>
    <ModelType>Poromechanics_FEM</ModelType>
    <SimParameters> simParam.dat </SimParameters>
    <Geometry>singleCrack_grid.vtk</Geometry>
    <Gauss> 3 </Gauss>
    <Material>Materials/MaterialsList.dat</Material>
    <BoundaryConditions count="5">
      <File>BCs/z_fix.dat</File>
      <File>BCs/x_fix.dat</File>
      <File>BCs/y_fix.dat</File>
      <File>BCs/loadLeft.dat</File>
      <File>BCs/loadRight.dat</File>
    </BoundaryConditions>
    <OutState outFile="outTime.dat"  print="1" />
  </Domain>


</SimulationInput>
 
