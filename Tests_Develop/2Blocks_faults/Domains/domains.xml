<?xml version="1.0" encoding="UTF-8"?>
<SimulationInput>
  <!-- Domain 1 -->
  <Domain>
    <Name>Left_block_hexa_P0</Name>
    <ModelType>Poromechanics_FEM</ModelType>
    <SimParameters> simParam.dat </SimParameters>
    <Geometry>LeftBlock.vtk</Geometry>
    <Gauss> 3 </Gauss>
    <Material>Materials/MaterialsList.dat</Material>
    <BoundaryConditions count="3">
      <File>BCs/leftBot.dat</File>
      <File>BCs/leftLat.dat</File>
      <File>BCs/leftYFix.dat</File>
    </BoundaryConditions>
    <OutState outFile="outTime.dat"  print="1" />
  </Domain>

  <!-- Domain 2 -->
  <Domain>
    <Name>Right_block_hexa_P0</Name>
    <ModelType>Poromechanics_FEM</ModelType>
    <SimParameters> simParam.dat </SimParameters>
    <Geometry>RightBlock.vtk</Geometry>
    <Gauss> 3 </Gauss>
    <Material>Materials/MaterialsList.dat</Material>
    <BoundaryConditions count = "4">
      <File>BCs/rightBot.dat</File>
      <File>BCs/rightYFix.dat</File>
      <File>BCs/rightLatLoad.dat</File>
      <File>BCs/rightTopLoad.dat</File>
    </BoundaryConditions>
     <OutState outFile="outTime.dat"  print="1" />
  </Domain>
</SimulationInput>
 
