<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->

<Problem>

  <simParam>
    <Time
          Start="0."
          End ="1.0e0"
          DtInit="1.0"
          DtMin ="1e0"
          DtMax ="1e0"
          />
  </simParam>

  <Materials>
    <Solid cellTags = "1" name = "mat">

      <Constitutive>
        <Elastic
                 youngModulus = "1.0"
                 poissonRatio = "0.0"
                 />
      </Constitutive>

    </Solid>
  </Materials>

  <BoundaryConditions>

    <BC
        name = "fixBottom"
        type = "Dirichlet"
        entityType = "SurfBC"
        variable = "displacements"
        >

      <BCentities
                  surfaceTags = "1"
                  components = "x,y,z"
                  />

      <BCevent
               time = "0"
               value = "0"
               />

    </BC>


    <BC
        name = "load"
        type = "Neumann"
        entityType = "SurfBC"
        variable = "displacements"
        >

      <BCentities
                  surfaceTags = "2"
                  components = "z"
                  />

      <BCevent
               time = "0"
               value = "-1"
               />

    </BC>


  </BoundaryConditions>


  <Solver>
    <Poromechanics />

    <!--

<EmbeddedFractureMechanics
penaltyNormal = "1e8"
penaltyTangential = "1e8">
<Fracture
normal = "0,0,1"
origin = "0.5,0.5,0.5"
lengthVec = "1,0,0"
widthVec = "0,1,0"
dimensions = "1.1,1.1"
/>

</EmbeddedFractureMechanics>

-->

  </Solver>

  <Output
          saveHistory = "1"
          outputFile = "OUT/patch"
          matFileName = "OUT/historyPatch"
          printTimes = "1"
          />

</Problem>
