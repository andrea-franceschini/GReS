<?xml version="1.0" encoding="UTF-8"?>

<Solver>
  <Sedimentation>
    <domain>
      <grid
          type = "classic"
          division = "3,3,3"
          size = "1,1,1"/>
      <initial>
        <lay number = "1" materialFlag = "1" />
        <lay number = "2" materialFlag = "1" />
        <lay number = "3" materialFlag = "1" />
      </initial>
    </domain>
    <boundary>
      <BC name = "BordA"
          type = "Newmann"
          surface = "X0"
          variable = "pressure"
          value = "0.,0.,0." />
      <BC name = "BordB"
          type = "Newmann"
          surface = "XM"
          variable = "pressure"
          value = "0.,0.,0." />
      <BC name = "BordC"
          type = "Newmann"
          surface = "Y0"
          variable = "pressure"
          value = "0.,0.,0." />
      <BC name = "BordD"
          type = "Newmann"
          surface = "YM"
          variable = "pressure"
          value = "0.,0.,0." />
      <BC name = "Bottom"
          type = "Newmann"
          surface = "Z0"
          variable = "pressure"
          value = "0.,0.,0." />
      <BC name = "Top"
          type = "Dirichlet"
          surface = "ZM"
          variable = "pressure"
          value = "0." />
    </boundary>
    <sediment_map
        time = "0."
        division = "3,3"
        sediment_rate = "0.01,0.01,0.01,0.01,0.01,0.01,0.01,0.01,0.01"/>
    <!--<sediment_map
        time = "0."
        division = "3,4"
        sediment_rate = "Inputs/DomainBot/bottomLayer.vtk"/>
        sediment_rate = "0.01,0.01,0.01,0.01"/>
     -->
    <Output outputFile = "Results/Dam"/>
  </Sedimentation>
</Solver>


