<?xml version="1.0" encoding="UTF-8"?>
<!-- Preprocess input for non-conforming multi-domain simulation -->

<Problem>


  <simParam>
    <Time
          Start="0."
          End ="10.0"
          DtInit="1.0"
          DtMin ="1e-5"
          DtMax ="1.0"
          incrementFactor = "1.25"
          choppingFactor = "2" />

    <Solver
            RelativeTolerance = "1.e-10"
            AbsoluteTolerance = "1.e-8"
            />
  </simParam>

  <Domain fileName = "model04/04_domLeft.xml"/>

  <Domain fileName = "model04/04_domRight.xml"/>


  <Interface>
    <MeshTying
               multiplierType="P0">

      <Master domainId="2" surfaceTag="5"/>
      <Slave  domainId="1" surfaceTag="6"/>

      <Output
              outputFile = "model04/Output04/Interface"
              printTimes = "1,5,10"
              saveHistory = "1"
              matFileName = "model04/Output04/historyInterface"
              />

    </MeshTying>

  </Interface>

</Problem>
