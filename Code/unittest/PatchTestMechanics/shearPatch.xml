<?xml version="1.0" encoding="UTF-8"?>

<Problem>

  <simParam>
    <Time
          Start="0.0"
          End ="3e0"
          DtInit= "1e0"
          DtMax ="1e0"
          DtMin ="1.0"
          />

    <Solver
            AbsoluteTolerance = "1e-8"
            RelativeTolerance = "1e-9"
            MaxNLIteration = "10"

            />
  </simParam>


  <Materials>
    <Solid
           cellTags = " 1 ">
      <Constitutive>
        <Elastic
                 youngModulus = "1.0"
                 poissonRatio = "0.0"
                 />
      </Constitutive>

    </Solid>

  </Materials>

  <BoundaryConditions>

    <BC
        name = "fix_bottom"
        type = "Dirichlet"
        entityType = "SurfBC"
        variable = "displacements"
        >

      <BCentities
                  surfaceTags = "1"
                  components = "x,y,z"
                  />

      <BCevent
               time = "0.0"
               value = "0.0"
               />

    </BC>


    <BC
        name = "top_disp"
        type = "Dirichlet"
        entityType = "SurfBC"
        variable = "displacements"
        >

      <BCentities
                  surfaceTags = "2"
                  components = "x"
                  />

      <BCevent
               time = "0.0"
               value = "0.0"
               />


      <BCevent
               time = "5.0"
               value = "5.0"
               />

    </BC>

    <BC
        name = "top_fix"
        type = "Dirichlet"
        entityType = "SurfBC"
        variable = "displacements"
        >

      <BCentities
                  surfaceTags = "2"
                  components = "z"
                  />

      <BCevent
               time = "0.0"
               value = "0.0"
               />

    </BC>

  </BoundaryConditions>


  <Solver>
    <Poromechanics />


  </Solver>



</Problem>

