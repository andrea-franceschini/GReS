<?xml version="1.0" encoding="UTF-8"?>
<SimulationInput>
  <!-- Domain 1 -->
  <Domain>
    <Name>TopBlock</Name>
    <ModelType>Poromechanics_FEM</ModelType>
    <SimParameters> simParam.xml </SimParameters>
    <Geometry>topBlock.vtk</Geometry>
    <Gauss> 3 </Gauss>
    <Material>materials.xml</Material>
    <BoundaryConditions> constantSlidingTopBCs.xml </BoundaryConditions>
    <OutState outFile="outTime.dat"  print="1" />
  </Domain>

  <!-- Domain 2 -->
  <Domain>
    <Name>bottomBlock</Name>
    <ModelType>Poromechanics_FEM</ModelType>
    <SimParameters> simParam.xml </SimParameters>
    <Geometry>bottomBlock.vtk</Geometry>
    <Gauss> 3 </Gauss>
    <Material>materials.xml</Material>
    <BoundaryConditions> constantSlidingBottomBCs.xml </BoundaryConditions>
    <OutState outFile="outTime.dat"  print="1" />
  </Domain>
</SimulationInput>
 
