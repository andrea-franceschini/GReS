<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->
<Domains>
  <Domain>

    <Name> DeepAquifer </Name>

    <Geometry> ReservoirTest_Hexa.msh </Geometry>

    <ModelType>
      SinglePhaseFlow_FVTPFA
      Poromechanics_FEM
    </ModelType>

    <SimParameters> simParam.xml </SimParameters>

    <DoFManager> dof.dat </DoFManager>

    <Gauss> 2 </Gauss>

    <Material> materials.xml </Material>

    <BoundaryConditions> boundaryConditions.xml </BoundaryConditions>


    <OutState 
              printTimes= "0.5,1.5" 
              saveHistory = "1"/>
    
  </Domain>
</Domains>
