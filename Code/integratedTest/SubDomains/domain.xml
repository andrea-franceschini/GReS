<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML input file is a shortcut to preprocess the model for each domain
in a nonConforming simulation.
-->
<Problem>

  <simParam fileName = "simParam.xml" />
  
  <Domain>
    <Geometry fileName= "ReservoirTest_Hexa.msh" />
    <Gauss nGP = "2" />
    <Materials fileName = "materials.xml"/>
    <BoundaryConditions fileName = "boundaryConditions.xml" />
    <Output
            printTimes= "0,0.5,1.5"
            saveHistory = "1"
    />

    <Solver>
      <BiotFullySaturated>
        <Poromechanics targetRegions = "1,2,3" />
        <SinglePhaseFlowFVTPFA targetRegions = "2,3" />
      </BiotFullySaturated>
    </Solver>
  </Domain>

</Problem>
